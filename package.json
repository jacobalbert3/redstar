{
  "name": "redstar",
  "version": "1.0.0",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "cross-env NODE_ENV=development nodemon -r dotenv/config server.js dotenv_config_path=.env.local",
    "dev:client": "webpack serve --mode development --port 9000",
    "build": "webpack --mode production",
    "start": "node server.js",
    "init-db": "node -r dotenv/config src/db/init.cjs dotenv_config_path=.env.local",
    "init-db:prod": "NODE_ENV=production node src/db/init.cjs",
    "test": "cross-env NODE_ENV=test jest --detectOpenHandles --forceExit",
    "test:server": "cross-env NODE_ENV=test nodemon -r dotenv/config server.js dotenv_config_path=.env.test",
    "test:ci": "npm run test:init-db && npm run test:server",
    "heroku-postbuild": "npm run build",
    "cypress:run": "cypress run"
  },
  "author": "Jacob Albert",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.3",
    "cheerio": "^1.0.0",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "emoji-regex": "^10.3.0",
    "express": "^4.21.2",
    "express-validator": "^7.2.0",
    "ioredis": "^5.4.2",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "mapbox-gl": "^3.9.0",
    "node-fetch": "^2.7.0",
    "pg": "^8.11.3",
    "pg-pool": "^3.6.1",
    "pg-promise": "^11.10.2",
    "prop-types": "^15.8.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.1.1",
    "rss-parser": "^3.13.0",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "sqlite3": "^5.1.7",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.26.3",
    "babel-loader": "^9.2.1",
    "concurrently": "^9.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "cypress": "^14.0.0",
    "jest": "^29.0.0",
    "nodemon": "^3.1.9",
    "style-loader": "^4.0.0",
    "supertest": "^6.0.0",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1",
    "webpack-dev-server": "^4.15.0"
  }
}
